{
  "name": "eleniarvanitis.com",
  "repository": "github.com/DatGreekChick/personal",
  "version": "2.0.0",
  "description": "My personal website featuring my portfolio and writing pieces.",
  "main": "main.js",
  "sideEffects": false,
  "scripts": {
    "build": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "clean": "rm public/*.bundle.js* public/*.hot-update.js*",
    "deploy": "npm run build:prod && npx firebase deploy",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "npm run lint --fix",
    "prettier": "npx prettier --write \"./**/*.{js,jsx,md}\"",
    "test": "jest",
    "start": "node dev",
    "watch": "npm run build -- -w"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "arrowParens": "avoid",
    "proseWrap": "always"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run prettier --",
      "npm run lint:fix --",
      "git add --"
    ],
    "*.{md,css}": [
      "npm run prettier --",
      "git add --"
    ]
  },
  "author": "Eleni Arvanitis Konior",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.13",
    "@babel/plugin-proposal-function-sent": "^7.12.13",
    "@babel/plugin-proposal-json-strings": "^7.13.8",
    "@babel/plugin-proposal-numeric-separator": "^7.12.13",
    "@babel/plugin-proposal-throw-expressions": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@hot-loader/react-dom": "^17.0.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "chalk": "^4.1.0",
    "compression-webpack-plugin": "^8.0.0",
    "css-loader": "^5.2.1",
    "eslint": "^7.24.0",
    "eslint-plugin-react": "^7.23.2",
    "file-loader": "^6.2.0",
    "firebase-tools": "^9.9.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "jest": "^27.0.2",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1",
    "react-hot-loader": "^4.13.0",
    "strip-ansi": "^6.0.0",
    "style-loader": "^2.0.0",
    "through2": "^4.0.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.32.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "workbox-webpack-plugin": "^6.1.5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "emailjs-com": "^3.0.2",
    "firebase": "^8.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-firebase-hooks": "^3.0.3",
    "react-recaptcha-v3": "^2.0.1",
    "react-router-dom": "^5.2.0",
    "react-text-transition": "^1.3.0",
    "react-toast-notifications": "^2.4.3",
    "styled-components": "^5.2.3"
  }
}
