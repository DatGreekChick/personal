{
  "name": "eleniarvanitis.com",
  "repository": "github.com/DatGreekChick/personal",
  "version": "2.0.0",
  "description": "My personal website featuring my portfolio and writing pieces.",
  "main": "main.js",
  "scripts": {
    "build": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "clean": "rm public/*.bundle.js public/*.hot-update.js*",
    "deploy": "npm run build:prod && npx firebase deploy",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "npm run lint --fix",
    "prepare": "firebase use elenicodes",
    "prettier": "npx prettier --write \"./**/*.{js,jsx,md}\"",
    "test": "jest",
    "start": "node dev",
    "watch": "npm run build -- -w"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "arrowParens": "avoid",
    "proseWrap": "always"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run prettier --",
      "npm run lint:fix --",
      "git add --"
    ],
    "*.{md,css}": [
      "npm run prettier --",
      "git add --"
    ]
  },
  "author": "Eleni Arvanitis Konior",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.13",
    "@babel/plugin-proposal-function-sent": "^7.14.5",
    "@babel/plugin-proposal-json-strings": "^7.13.8",
    "@babel/plugin-proposal-numeric-separator": "^7.12.13",
    "@babel/plugin-proposal-throw-expressions": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.15.3",
    "@hot-loader/react-dom": "^17.0.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.2",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "firebase-admin": "^9.11.1",
    "firebase-tools": "^9.16.5",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "react-async-script": "^1.2.0",
    "react-hot-loader": "^4.13.0",
    "react-test-renderer": "^17.0.2",
    "style-loader": "^3.2.1",
    "through2": "^4.0.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.51.1",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0",
    "workbox-webpack-plugin": "^6.2.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.15",
    "emailjs-com": "^3.2.0",
    "firebase": "^8.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-firebase-hooks": "^3.0.3",
    "react-recaptcha-v3": "^2.0.1",
    "react-router-dom": "^5.2.0",
    "react-text-transition": "^1.3.0",
    "react-toast-notifications": "^2.5.1",
    "strip-ansi": "^6.0.0",
    "styled-components": "^5.2.3",
    "undefined": "^0.1.0"
  }
}
