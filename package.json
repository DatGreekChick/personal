{
  "name": "eleniarvanitis.com",
  "repository": "github.com/DatGreekChick/personal",
  "version": "2.0.0",
  "description": "My personal website featuring my portfolio and writing pieces.",
  "main": "main.js",
  "scripts": {
    "build": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "clean": "rm public/*.bundle.js public/*.hot-update.js*",
    "deploy": "npm run build:prod && npx firebase deploy",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "npm run lint --fix",
    "prepare": "firebase use elenicodes",
    "prettier": "npx prettier --write \"./**/*.{js,jsx,md}\"",
    "test": "jest",
    "start": "node dev",
    "watch": "npm run build -- -w"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "arrowParens": "avoid",
    "proseWrap": "always"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run prettier --",
      "npm run lint:fix --",
      "git add --"
    ],
    "*.{md,css}": [
      "npm run prettier --",
      "git add --"
    ]
  },
  "author": "Eleni Arvanitis Konior",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/plugin-proposal-function-sent": "^7.16.7",
    "@babel/plugin-proposal-json-strings": "^7.16.7",
    "@babel/plugin-proposal-numeric-separator": "^7.16.7",
    "@babel/plugin-proposal-throw-expressions": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "^7.17.2",
    "@hot-loader/react-dom": "^17.0.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^2.0.6",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.2",
    "css-loader": "^6.7.1",
    "eslint": "^8.11.0",
    "eslint-plugin-react": "^7.28.0",
    "file-loader": "^6.2.0",
    "firebase-admin": "^10.0.2",
    "firebase-tools": "^10.1.5",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "react-async-script": "^1.2.0",
    "react-hot-loader": "^4.13.0",
    "react-test-renderer": "^17.0.2",
    "strip-ansi": "^6.0.1",
    "style-loader": "^3.3.1",
    "through2": "^4.0.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.68.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "workbox-webpack-plugin": "^6.4.2"
  },
  "dependencies": {
    "@emailjs/browser": "^3.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "firebase": "^9.6.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-firebase-hooks": "^5.0.2",
    "react-recaptcha-v3": "^2.0.1",
    "react-router-dom": "^6.2.1",
    "react-text-transition": "^1.3.0",
    "react-toast-notifications": "^2.5.1",
    "styled-components": "^5.3.3"
  }
}
